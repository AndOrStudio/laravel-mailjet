{
    "name": "moltencore/laravel-mailjet",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel", "mailjet", "mailjet wrapper"],
    "license": "MIT",
    "type": "library",
    "homepage": "https://github.com/MoltenCoreIO/laravel-mailjet",
    "require": {
        "php": ">=5.6.4",
        "laravel/framework": "5.4.*",
        "laravel/tinker": "~1.0",
        "mailjet/mailjet-apiv3-php": "^1.2"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.7"
    },
    "autoload": {
        "psr-4": {
            "MoltenCore\\LaravelMailjet\\": "src/"
        }
    },
    "config": {
        "sort-packages": true,
        "bin-dir": "bin"
    }
}
